{
    "name": "tomazov/geoip2-update",
    "description": "Update GeoIP2/GeoLite2 databases from your script, program or via composer.",
    "keywords": [
        "geoip",
        "geoip2",
        "geolite2",
        "geolocation",
        "maxmind"
    ],
    "type": "library",
    "require": {
        "php": ">=5.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "suggest": {
        "ext-zip": "Required for updating the CSV databases."
    },
    "autoload": {
        "psr-4": {
            "tomazov\\GeoIP2Update\\": "src/"
        },
        "classmap": [
            "vendor/tomazov/geoip2-update/src/Client.php",
            "vendor/tomazov/geoip2-update/src/ComposerClient.php",
            "vendor/tomazov/geoip2-update/src/ComposerConsole.php"
        ]
    }
}
